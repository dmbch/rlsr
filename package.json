{
  "name": "rlsr",
  "version": "3.2.0",
  "description": "create npm releses and changelogs from a multi repo",
  "main": "src/index.js",
  "bin": {
    "rlsr": "./bin/rlsr.js"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.com/"
  },
  "scripts": {
    "c": "git-cz",
    "test": "npm run unittest && npm run lint",
    "unittest": "jest",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/matthias-reis/rlsr.git"
  },
  "keywords": [
    "npm",
    "release",
    "changelog",
    "conventional-changelog"
  ],
  "author": "Matthias Reis",
  "license": "MIT",
  "devDependencies": {
    "commitizen": "3.1.1",
    "cz-conventional-changelog": "2.0.0",
    "eslint": "^4.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "^24.8.0",
    "semistandard": "^11.0.0"
  },
  "dependencies": {
    "conventional-commits-parser": "1.3.0",
    "git-latest-semver-tag": "1.0.2",
    "git-raw-commits": "1.1.2",
    "glob": "7.1.2",
    "npmlog": "4.1.2",
    "ramda": "0.26.1",
    "recursive-diff": "^0.1.2",
    "semver": "5.4.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "rlsr": {
    "verbose": true
  }
}
