{
  "name": "rlsr",
  "version": "1.0.0-beta.3",
  "description": "create npm releses and changelogs from a multi repo",
  "main": "src/index.js",
  "bin": {
    "rlsr": "./bin/rlsr.js"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.com/"
  },
  "scripts": {
    "c": "git-cz",
    "test": "npm run unittest && npm run lint",
    "unittest": "jest",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/matthias-reis/rlsr.git"
  },
  "keywords": ["npm", "release", "changelog", "conventional-changelog"],
  "author": "Matthias Reis",
  "license": "MIT",
  "devDependencies": {
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.0.0",
    "jest": "^19.0.2"
  },
  "dependencies": {
    "conventional-commits-parser": "1.3.0",
    "eslint": "^3.18.0",
    "git-latest-semver-tag": "1.0.2",
    "git-raw-commits": "1.1.2",
    "glob": "7.1.1",
    "npmlog": "4.0.2",
    "ramda": "0.23.0",
    "recursive-diff": "^0.1.2",
    "semistandard": "^10.0.0",
    "semver": "5.3.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "rlsr": {
    "verbose": true
  }
}
