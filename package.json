{
  "name": "rlsr",
  "version": "3.2.0",
  "description": "create npm releses and changelogs from a multi repo",
  "main": "src/index.js",
  "bin": {
    "rlsr": "./bin/rlsr.js",
    "rlsr4": "./bin/rlsr4.js"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.com/"
  },
  "scripts": {
    "c": "git-cz",
    "rlsr4": "node ./bin/rlsr4",
    "test": "yarn jest && yarn lint",
    "lint": "eslint .",
    "ts:watch": "yarn tsc --watch",
    "ts:compile": "yarn tsc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/matthias-reis/rlsr.git"
  },
  "keywords": [
    "npm",
    "release",
    "changelog",
    "conventional-changelog"
  ],
  "author": "Matthias Reis",
  "license": "MIT",
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/jest": "^26.0.19",
    "@types/lodash": "^4.14.167",
    "@types/node": "^14.14.20",
    "commitizen": "4.2.2",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^7.17.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "jest": "^26.6.3",
    "semistandard": "^16.0.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@schemastore/package": "^0.0.6",
    "@types/chalk": "^2.2.0",
    "@types/conventional-commits-parser": "^3.0.1",
    "@types/cosmiconfig": "^6.0.0",
    "@types/yargs": "^15.0.12",
    "chalk": "^4.1.0",
    "conventional-commits-parser": "3.2.0",
    "cosmiconfig": "^7.0.0",
    "eslint-config-semistandard": "^15.0.1",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.2.1",
    "git-latest-semver-tag": "1.0.2",
    "git-raw-commits": "2.0.9",
    "glob": "7.1.6",
    "lodash": "^4.17.20",
    "npmlog": "4.1.2",
    "prettier": "^2.2.1",
    "ramda": "0.27.1",
    "recursive-diff": "^1.0.8",
    "semver": "7.3.4",
    "simple-git": "^2.31.0",
    "yargs": "^16.2.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "rlsr": {
    "debug": true,
    "registry": "https://nexus.dc.xing.com/repository/npm-publish/"
  },
  "jest": {
    "preset": "ts-jest",
    "testPathIgnorePatterns": [
      "<rootDir>/build"
    ]
  }
}
